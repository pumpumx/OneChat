{
    "version": 2,
    "builds": [
      {
        "src": "src/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "client/build/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "src/$1.js"
      },
      {
        "src": "/(.*)",
        "dest": "client/build/index.html"
      }
    ]
  }
  